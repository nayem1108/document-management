{
  "name": "nayem1108/document-management",
  "description": "Document management tools",
  "keywords": [
    "nayem1108",
    "document-management"
  ],
  "homepage": "https://github.com/nayem1108/document-management",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Nayem uddin",
      "email": "nayem110899@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Nayem1108\\DocumentManagement\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nayem1108\\DocumentManagement\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nayem1108\\DocumentManagement\\DocumentManagementServiceProvider"
      ],
      "aliases": {
        "DocumentManagement": "Nayem1108\\DocumentManagement\\DocumentManagementFacade"
      }
    }
  }
}
